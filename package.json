    "@babel/preset-react": "^7.6.3",
    "babel-loader": "^8.0.6",
    "express": "^4.17.1",
    "file-loader": "^4.2.0",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-router-dom": "^5.1.2",
    "react-transition-group": "^4.3.0",
    "styled-components": "^4.4.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9",
    "webpack-node-externals": "^1.7.2"
  },
  "devDependencies": {
    "nodemon": "^1.19.4"
  }
